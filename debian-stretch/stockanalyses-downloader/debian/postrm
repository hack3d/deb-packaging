#!/bin/sh
# postrm script for stockanalyses-downloader
#
# see: dh_installdeb(1)

set -e

case "$1" in
  purge)
    exit 0
  ;;

  remove|upgrade|failed-upgrade|abort-install|abort-upgrade|disappear)
    if [ -f /opt/stockanalyses-downloader/lib/python3.5/site-packages/downloader/config ]; then
      rm -f /opt/stockanalyses-downloader/lib/python3.5/site-packages/downloader/config
    fi
  ;;

  *)
    echo "postrm called with unknown argument \`$1'" >&2
    exit 1
  ;;

esac

# dh_installdeb will replace this with shell code automatically
# generated by other debhelper scripts.

#DEBHELPER#

exit 0